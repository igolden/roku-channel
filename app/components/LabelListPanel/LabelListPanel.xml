<!-- This is a panel that has a list inside of it and handles proper focusing of it.
	List is accessible via interface field "list" so you can customize it easily. -->
<component name="LabelListPanel" extends="Panel" xsi:noNamespaceSchemaLocation="https://devtools.web.roku.com/schema/RokuSceneGraph.xsd">

	<interface>
		<!-- provides acess to items list -->
		<field id="list" type="node"/>
	</interface> 
	
	<script type="text/brightscript" uri="pkg:/components/LabelListPanel/LabelListPanel.brs"/>
	
	<children>
		<LabelList
			id="List"
			numRows="15" />
	</children>
</component>
